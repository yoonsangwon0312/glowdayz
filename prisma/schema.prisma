generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model member {
  mbr_idx      Int     @id @default(autoincrement())
  mbr_type     Int?
  mbr_id       String? @db.VarChar(255)
  mbr_password String? @db.VarChar(255)
  mbr_level    Int?    @default(0)
  mbr_name     String? @db.VarChar(255)
  mbr_email    String? @db.VarChar(255)
  mbr_phone    String? @db.VarChar(255)
  mbr_call     String? @db.VarChar(255)
  mbr_memo     String? @db.VarChar(255)
  mbr_url      String? @db.VarChar(255)
  mbr_category Int?
}
